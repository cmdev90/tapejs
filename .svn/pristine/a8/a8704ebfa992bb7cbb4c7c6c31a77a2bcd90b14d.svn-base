#banner{
  margin-bottom : 20px;
}

.project .badge{
  margin: 5px;
}

.notif {
  padding:5px;
  border: 1px solid whiteSmoke;
  border-radius: 5px;
  min-height : 40px;
  margin: 10px;
  width:95%;
}
.notif img {
  width:60px;
  height:60px;
  float:left;
  border-radius: 5px;
  border: 1px solid whiteSmoke;
}
.notif-body {
  margin-left:70px;
}
.notif-title{
  padding-bottom: 5px;
}
.notif-message {
  font-size: 11px;
  line-height: 16px;
  max-height: 50px;
  overflow: hidden;
  margin-bottom: 5px;
}
.notif-info {
  font-size: 11px;
  line-height: 15px;
  text-align: right;
  padding-right: 10px;
}
.notif:hover{
  cursor: pointer;
  border: #9ecaed;
  box-shadow: 0 0 10px #9ecaed;
}



.form-actions-changed{
  text-align: right;
}
.form-actions-changed .btn{
  width: 125px;
}
.form-actions-changed .btn:first-child{
  margin-right: 10px;
}
.doc-list{
  height:300px;
  overflow: auto;
  border:1px solid whiteSmoke;
}
.doc{
  margin: 5px 10px;
  border-radius: 5px;
  border: 1px solid whiteSmoke;
  padding:5px;
  min-height:50px;
}
.doc:hover{
  cursor: pointer;
  border: #9ecaed;
  box-shadow: 0 0 10px #9ecaed; 
}
.doc img{
  width:40px;
  height:40px;
  float: left;
}
.doc .title{
  margin-left: 45px;
  font-weight: bold;
  overflow: hidden;
  text-overflow: ellipsis;
  -o-text-overflow: ellipsis;
  white-space: nowrap;
}
.doc .timestamp{
  text-align: right;
  font-size: 11px;
  margin-top: 5px;
  font-style: italic;
}
.poject-feed{
  height: 170px;
  overflow: auto;
  border: 1px solid whiteSmoke;
  padding: 5px;
}
.poject-feed .notif{
  min-height : 60px;
}
.indicator-modal{
  margin-left: 0;
  left: 5%;
  z-index: 1050;
  width: 90%;
}
.modal-responsive{
  margin-left: 0;
  left: 15%;
  z-index: 1050;
  width: 70%;
}
.modal-responsive .modal-body{
  max-height:480px;
}
.file-modal-table td{
  padding: 5px;
}
.file-modal-table td div{
  min-height: 150px;
  overflow: auto;
}




