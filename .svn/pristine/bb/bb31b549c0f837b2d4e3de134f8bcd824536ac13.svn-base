<div class="pull-right">  <button class="btn btn-danger delete-object">Delete</button></div><br><div><h4>{{type}}: {{name}}</h4><b>Description : </b> {{description}}<hr><div class="pull-right"><button data-link="/indicator/object/{{id}}" class="addIndicator btn">Add Indicator</button></div><h4>Indicators</h4><div id="indicator_accordian" class="accordion">{{#each indicators}}<div class="accordion-group"><div class="accordion-heading"><a data-toggle="collapse" data-parent="#accordion2" href="#collapse{{this.id}}" class="accordion-toggle">{{this.name}}</a></div><div id="collapse{{this.id}}" class="accordion-body collapse"><div class="accordion-inner"><p> <b>Description : </b> {{this.description}}</p><p> <b>Means of Verification :</b> {{this.mov}}</p><p> <b>Assumptions/Risk :</b> {{this.risk}}</p></div></div></div>{{/each}}</div></div>